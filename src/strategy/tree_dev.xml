        <Sequence name="root_sequence">
            <MoveAhead name="move_ahead"
                       moveForward="true"
                       distance="10"
                       obstacleInFront="{obstacleInFront}" obstacleBehind="{obstacleBehind}"
            />
        </Sequence>
    </BehaviorTree>
</root>
